generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id           Int @id @default(autoincrement())
  name         String
  description  String  
  completed    Boolean @default(false)
  createdAt    DateTime? @default(now())
}